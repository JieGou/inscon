<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <!-- CACHING -->
  <system.runtime.caching>
    <memoryCache>
      <namedCaches>
        <add name="EnterpriseCache" cacheMemoryLimitMegabytes="2048" physicalMemoryLimitPercentage="40" pollingInterval="00:04:01"/>
      </namedCaches>
    </memoryCache>
  </system.runtime.caching>

  <!-- DATABASE CONNECTION STRINGS -->
  <connectionStrings>
    <add name="EnterpriseDb" connectionString="Data Source=inscon1.iss.local;Initial Catalog=EnterpriseDb;Trusted_Connection=true;Application Name=Inscon" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings/>
  
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
</configuration>